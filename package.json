{
  "name": "wirvi-roka",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "homepage": "https://wirvi.com",
  "repository": {
    "url": "https://github.com/777wills/wirvi-roka",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build-storybook": "rimraf docs/ && build-storybook && npm run rename",
    "build": "npm run delete-dist && tsc && npm run copy-files && npm run clean-dist",
    "delete-dist": "rimraf dist/",
    "clean-dist": "rimraf dist/stories/ && rimraf dist/test",
    "copy-files": "copyfiles -u 1 src/**/*.scss dist/",
    "rename": "renamer --find storybook-static --replace docs *",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/react": "6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.5",
    "bulma": "0.9.3",
    "copyfiles": "2.4.1",
    "esbuild": "0.14.38",
    "esbuild-jest": "0.5.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "renamer": "4.0.0",
    "rimraf": "3.0.2",
    "sass": "1.50.1",
    "semantic-release": "19.0.2",
    "storybook-addon-sass-postcss": "0.1.1",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.3",
    "web-vitals": "2.1.4"
  },
  "dependencies": {
    "classnames": "2.3.1"
  },
  "peerDependencies": {
    "bulma": ">=0.9.3",
    "react": ">=17",
    "typescript": ">=4.6.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
