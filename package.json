{
  "name": "wirvi-roka",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "homepage": "https://wirvi.com",
  "repository": {
    "url": "https://github.com/777wills/wirvi-roka",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build-storybook": "build-storybook && npm run rename",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.scss dist/",
    "rename": "renamer --find storybook-static --replace docs *",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "bulma": "^0.9.3",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.14.38",
    "esbuild-jest": "^0.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.50.1",
    "semantic-release": "^19.0.2",
    "storybook-addon-sass-postcss": "^0.1.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "bulma": ">=0.9.3",
    "react": ">=18",
    "typescript": ">=4.6.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
